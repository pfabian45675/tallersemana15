<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <display-name>Empleados</display-name>
    
    <servlet>
        <servlet-name>ClienteServlet</servlet-name>
        <servlet-class>com.example.ClienteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClienteServlet</servlet-name>
        <url-pattern>/cliente</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProductoServlet</servlet-name>
        <servlet-class>com.example.ProductoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductoServlet</servlet-name>
        <url-pattern>/producto</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VentaServlet</servlet-name>
        <servlet-class>com.example.VentaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VentaServlet</servlet-name>
        <url-pattern>/venta</url-pattern>
    </servlet-mapping>
</web-app>
